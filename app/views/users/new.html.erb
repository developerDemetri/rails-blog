<html>
  <body>
      <%= form_for :user, url: users_path do |f| %>
      <p>
        <%= f.label :username %>
      </p>
      <p>
        <%= f.text_field :username %>
      </p>
      <p>
        <%= f.label :password %>
      </p>
      <p>
        <%= f.password_field :password %>
      </p>
      <p>
        <%= f.label :password_confirmation %>
      </p>
      <p>
        <%= f.password_field :password_confirmation %>
      </p>
      <p>
        <%= f.submit "Register", :class => 'btn waves-effect waves-light' %>
      </p>
    <% end %>
  </body>
</html>
